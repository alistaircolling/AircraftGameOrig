	<fx:Declarations>
		<s:Fade id="fadeIn" alphaFrom="0" alphaTo="1" duration=".6"/>
		<s:Fade id="fadeOut" alphaFrom="1" alphaTo="0" duration=".6"/>
	</fx:Declarations> 
	<fx:Script>
		<![CDATA[
			
			public function showPlanes( n:uint ):void{
				
				for (var i:uint = 1; i<=numElements; i++){
					var im:InAirPlane = this["p"+i] as InAirPlane;
					im.alpha = 0;	
					
				}
				for (var j:uint = 1; j<=n;j++){
					var im:InAirPlane = this["p"+j] as InAirPlane;
					im.alpha = 1;
				}
				
			}
			
		]]>
	</fx:Script>
